	<oid>c0c010c0-d34d-b33f-f00d-111111111111</oid>
	<propertyModification modificationType="add">
		<value>
			<assignment>
				<accountConstruction>
					<resourceRef oid="10000000-0000-0000-0000-000000000003"/>
					<attribute>
						<ref>ri:o</ref>
						<value>Pirate Brethren, Inc.</value>
					</attribute>
					<attribute>
						<ref>ri:l</ref>
						<value>Caribbean</value>
					</attribute>
				</accountConstruction>
			</assignment>
		</value>
	</propertyModification>
</objectModification>
