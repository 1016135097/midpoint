<!-- Import test file with references between objects -->

<objects xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:piracy='http://midpoint.evolveum.com/xml/ns/samples/piracy'>
	
	<!-- should be "connector" instead of "object". But !@#@#!!!! Metro JAXB ignores it. -->
	<object xsi:type="ConnectorType">
	   <name>ICF org.identityconnectors.ldap.LdapConnector</name>
	   <connectorType>org.identityconnectors.ldap.LdapConnector</connectorType>
	   <connectorVersion>1.0.5531</connectorVersion>
	   <namespace>http://midpoint.evolveum.com/xml/ns/resource/icf/icf1-org.identityconnectors.ldap-1.0.5531-org.identityconnectors.ldap.LdapConnector</namespace>
	</object>

	<resource>
		<name>Sample Resource</name>
		<connectorRef type="ConnectorType">
	      <and>
	        <type uri="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd#ConnectorType"/>
	        <equal>
	          <value>
	              <connectorType>org.identityconnectors.ldap.LdapConnector</connectorType>
	          </value>
	        </equal>
	      </and>
		</connectorRef>
		<namespace>http://foo.bar.com/</namespace>
	</resource>

	<user>
        <name>jack</name>
        <extension>
            <piracy:ship>Black Pearl</piracy:ship>
        </extension>
        <fullName>Cpt. Jack Sparrow</fullName>
        <givenName>Jack</givenName>
        <familyName>Sparrow</familyName>
        <honorificPrefix>Cpt.</honorificPrefix>
    </user>

</objects>
