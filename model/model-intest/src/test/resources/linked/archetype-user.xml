<archetype xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
           oid="c46b1bcc-af43-44ee-a107-71f36e952cc5">
    <name>user</name>
    <inducement>
        <focusMappings>
            <mapping>
                <documentation>Removes all orphaned organizationalUnit values. Should be executed with all
                    other assigned mappings, therefore it is placed here.</documentation>
                <strength>strong</strength>
                <expression>
                    <script>
                        <code>null</code>
                    </script>
                </expression>
                <target>
                    <path>organizationalUnit</path>
                    <set>
                        <predefined>all</predefined>
                    </set>
                </target>
            </mapping>
        </focusMappings>
    </inducement>
</archetype>
