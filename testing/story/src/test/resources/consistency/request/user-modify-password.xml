<!-- Object modification description that is changing user "jack" by adding a new account -->

<objectModification
	xmlns='http://midpoint.evolveum.com/xml/ns/public/common/api-types-3'
	xmlns:c='http://midpoint.evolveum.com/xml/ns/public/common/common-3'
	xmlns:t="http://prism.evolveum.com/xml/ns/public/types-3">
	<oid>c0c010c0-d34d-b33f-f00d-111111111111</oid>
	<!-- Change jack's password -->
	<modification>
		<t:modificationType>replace</t:modificationType>
		<t:path>c:credentials/c:password</t:path>
		<t:value>
			<c:value>
				<c:clearValue>butUnd3dM4yT4lkAL0t</c:clearValue>
			</c:value>
		</t:value>
	</modification>
</objectModification>
