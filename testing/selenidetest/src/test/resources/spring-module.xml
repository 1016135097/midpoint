<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="baseTest" class="com.evolveum.midpoint.testing.selenide.tests.BaseTest" autowire-candidate="true"/>
    <bean id="createAccountTest" class="com.evolveum.midpoint.testing.selenide.tests.account.CreateAccountTest"
          autowire-candidate="true"/>
    <bean id="createUserTest" class="com.evolveum.midpoint.testing.selenide.tests.user.CreateUserTest"
          autowire-candidate="true"/>
    <bean id="importResourceTest" class="com.evolveum.midpoint.testing.selenide.tests.resource.ImportResourceTest"
          autowire-candidate="true"/>
    <bean id="checkResourceConnectionTest"
          class="com.evolveum.midpoint.testing.selenide.tests.resource.CheckResourceConnectionTest"
          autowire-candidate="true"/>
    <bean id="userUtil" class="com.evolveum.midpoint.testing.selenide.tests.user.UserUtil" autowire-candidate="true">
        <property name="testUserName" value="TestUser"/>
    </bean>
    <bean id="resourceUtil" class="com.evolveum.midpoint.testing.selenide.tests.resource.ResourceUtil"
          autowire-candidate="true">
        <property name="testResourceName" value="Localhost OpenDJ (no extension schema)"/>
    </bean>
</beans>