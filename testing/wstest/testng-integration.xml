<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="unit" parallel="false">
<listeners>
		<listener class-name="com.evolveum.midpoint.tools.testng.AlphabeticalMethodInterceptor"/>
	</listeners>
	
    <test name="WSTest" parallel="false" verbose="10" enabled="true">
        <classes>
            <class name="com.evolveum.midpoint.testing.wstest.TestWSSecurity"/>
