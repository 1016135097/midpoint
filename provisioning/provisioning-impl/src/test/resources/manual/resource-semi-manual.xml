<resource oid="b6eb1e50-2414-11e7-bf12-579151795f29"
		xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
        xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:conf="http://midpoint.evolveum.com/xml/ns/public/connector/builtin-1/bundle/com.evolveum.midpoint.provisioning.ucf.impl.builtin/ManualConnector"
        xmlns:csvconf="http://midpoint.evolveum.com/xml/ns/public/connector/builtin-1/bundle/com.evolveum.midpoint.provisioning.ucf.impl.builtin/ManualConnector"
        xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance-3"
        xmlns:cap="http://midpoint.evolveum.com/xml/ns/public/resource/capabilities-3">
								  
	<name>Semi-Manual Resource</name>
	
	<connectorRef oid="will-be-supplied-by-the-test-code"/>
	<connectorConfiguration>
		<conf:defaultAssignee>administrator</conf:defaultAssignee>
	</connectorConfiguration>
	
	<additionalConnector>
		<name>csv</name>
		<connectorRef oid="will-be-supplied-by-the-test-code"/>
		<connectorConfiguration>
			<conf:defaultAssignee>administrator</conf:defaultAssignee>
		</connectorConfiguration>
	</additionalConnector>
	
	<!-- Schema definition comes from the CSV file -->

	<!-- No schemaHandling -->
	
	<!-- No capabilities. Connector should generate those. -->
	
	<consistency>
		<pendingOperationGracePeriod>PT15M</pendingOperationGracePeriod>
	</consistency>
	
</resource>
