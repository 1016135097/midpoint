            <class name="com.evolveum.midpoint.schema.util.XsdTypeConverterTest"/>
            <class name="com.evolveum.midpoint.schema.util.JAXBUtilTest"/>
            <class name="com.evolveum.midpoint.schema.TestSchemaRegistry"/>
            <class name="com.evolveum.midpoint.schema.TestSchemaProcessorWithRegistry"/>
            <class name="com.evolveum.midpoint.schema.TestJaxbSanity"/>
