<?xml version="1.0" encoding="UTF-8"?>
<expression xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd">
	<language>http://midpoint.evolveum.com/xml/ns/public/expression/language#ECMAScript</language>
    <variable>
    	<name>foo</name>
    	<value>FOO</value>
    </variable>
    <variable>
    	<name>bar</name>
    	<value>BAR</value>
    </variable>
	<code>
		foo + bar;
	</code>
</expression>
