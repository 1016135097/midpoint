<?xml version="1.0" encoding="UTF-8"?>
<valueConstruction 
		xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-2"
		xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-2" 
		xmlns:x="http://example.com/xxx"
		xmlns:y="http://example.com/yyy">
	<expression>
		<variable>
			<name>x:foo</name>
	    	<value>Captain</value>
	    </variable>
	    <variable>
	    	<name>y:jack</name>
	    	<objectRef oid="c0c010c0-d34d-b33f-f00d-111111111111" type="UserType"/>
	    </variable>
		<code xmlns:x="http://example.com/xxx">
			concat($x:foo,' ',$y:jack/c:name)
		</code>
	</expression>
</valueConstruction>
