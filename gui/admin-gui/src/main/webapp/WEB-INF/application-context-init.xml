<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
     http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
     http://www.springframework.org/schema/context
     http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="initialSetup" class="com.evolveum.midpoint.init.InitialSetup" init-method="init" scope="singleton">
		<property name="model" ref="modelController" />
	</bean>
	
	<bean name="modelInitialSetup" class="com.evolveum.midpoint.init.ModelInitialSetup" depends-on="loggingManager" init-method="init">
		<property name="model" ref="modelController" />
	</bean>
</beans>
