<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0">
	<namespace>http://midpoint.evolveum.com/xml/ns/custom-taglib/</namespace>
	<tag>
        <tag-name>autoForm</tag-name>
        <component>
            <component-type>AutoForm</component-type>
            <handler-class>com.evolveum.midpoint.web.component.form.AutoFormTag</handler-class>
        </component>
        <attribute>
            <name>bean</name>
            <required>true</required>
        </attribute>
        <attribute>
            <name>index</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>editable</name>
            <required>false</required>
        </attribute>
    </tag>
	<tag>
		<tag-name>button</tag-name>
		<component>
			<component-type>com.evolveum.midpoint.web.component.button.HtmlButton</component-type>
			<renderer-type>HtmlButtonRenderer</renderer-type>
		</component>
	</tag>
	<tag>
		<tag-name>inputSyntaxTextArea</tag-name>
		<component>
			<component-type>javax.faces.HtmlInputTextarea</component-type>
			<renderer-type>InputSyntaxTextAreaRenderer</renderer-type>
		</component>
	</tag>
	<tag>
		<tag-name>schemaMessage</tag-name>
		<component>
			<component-type>javax.faces.HtmlMessage</component-type>
			<renderer-type>schemaMessageRenderer</renderer-type>
		</component>
	</tag>
	<tag>
		<tag-name>topMenu</tag-name>
		<component>
			<component-type>HtmlTopMenu</component-type>
			<renderer-type>com.icesoft.faces.Form</renderer-type>
		</component>
	</tag>
	<tag>
		<tag-name>topMenuItem</tag-name>
		<component>
			<component-type>HtmlTopMenuItem</component-type>
			<renderer-type>com.icesoft.faces.Link</renderer-type>
		</component>
	</tag>
	<tag>
		<tag-name>leftMenu</tag-name>
		<component>
			<component-type>HtmlLeftMenu</component-type>
			<renderer-type>com.icesoft.faces.Form</renderer-type>
		</component>
	</tag>
	<tag>
		<tag-name>leftMenuContainer</tag-name>
		<component>
			<component-type>HtmlLeftMenuContainer</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>leftMenuItem</tag-name>
		<component>
			<component-type>HtmlLeftMenuItem</component-type>
			<renderer-type>com.icesoft.faces.Link</renderer-type>
		</component>
	</tag>
	<tag>
		<tag-name>xmlInput</tag-name>
		<component>
			<component-type>AceXmlInput</component-type>
		</component>
		<attribute>
            <name>id</name>
            <required>true</required>
        </attribute>
        <attribute>
            <name>value</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>width</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>height</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>readonly</name>
            <required>false</required>
        </attribute>
	</tag>
</facelet-taglib>
