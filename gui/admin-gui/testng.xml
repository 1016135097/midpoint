<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="false">
	<listeners>
		<listener class-name="com.evolveum.midpoint.tools.testng.TestMethodLoggerListener"/>
	</listeners>
	<test name="Spring context test" preserve-order="false">
		<classes>
			<class name="com.evolveum.midpoint.web.test.SpringApplicationContextTest" />
		</classes>
	</test>
	<test name="Resource manager" preserve-order="false">
		<classes>
			<class name="com.evolveum.midpoint.web.test.ResourceManagerImplMock" />
		</classes>
	</test>
	<test name="Security" preserve-order="false">
		<classes>
			<class name="com.evolveum.midpoint.web.security.MidPointAuthenticationProviderTest" />
		</classes>
	</test>
	<test name="User Manager" preserve-order="false">
		<classes>
			<class name="com.evolveum.midpoint.web.model.impl.UserManagerImplTest" />
		</classes>
	</test>
	<test name="Controler" preserve-order="false" parallel="false">
		<classes>
			<class name="com.evolveum.midpoint.web.controller.AboutControllerTest" />
			<class name="com.evolveum.midpoint.web.controller.config.XPathDebugControllerTest" />
		</classes>
	</test>
</suite>
