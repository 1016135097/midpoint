<query xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd"
       xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-1.xsd"
        xmlns:t="http://prism.evolveum.com/xml/ns/public/types-2">
    <and>
        <equal>
            <t:value>
                <name>generic object</name>
            </t:value>
        </equal>
        <equal>
            <t:path>c:extension</t:path>
            <t:value>
                <p:intType xmlns:p="http://example.com/p">123</p:intType>
            </t:value>
        </equal>
    </and>
</query>
